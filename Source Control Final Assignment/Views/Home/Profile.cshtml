@model IEnumerable<Source_Control_Final_Assignment.Models.UserData>

@{
    ViewBag.Title = "Profile";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}
@{ 
    var fname = ViewData["fname"];
    var mname = ViewData["mname"];
    var lname = ViewData["lname"];
    var uname = ViewData["uname"];
    var age = ViewData["age"];
    var bday = ViewData["bday"];
    var address = ViewData["address"];
    var contact = ViewData["contact"];
    var email = ViewData["email"];
    var path = ViewData["path"];
}

<h2>Profile</h2>

<p>
    @uname's Data
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.fname)
        </th>
        <td>
            @fname
        </td>
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.mname)
        </th>
        <td>
            @mname
        </td>
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.lname)
        </th>
        <td>
            @lname
        </td>
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.uname)
        </th>
        <td>
            @uname
        </td>
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.age)
        </th>
        <td>
            @age
        </td>
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.bday)
        </th>
        <td>
            @bday
        </td>
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.address)
        </th>
        <td>
            @address
        </td>
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.contact)
        </th>
        <td>
            @contact
        </td>
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <td>
            @email
        </td>
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.image)
        </th>
        <td>
            <img src="@Url.Content(path.ToString())" alt=@uname width="300" height="300"/>
        </td>

    </tr>
</table>
