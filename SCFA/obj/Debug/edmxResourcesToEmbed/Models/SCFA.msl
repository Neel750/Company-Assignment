<?xml version="1.0" encoding="utf-8"?>
<Mapping Space="C-S" xmlns="http://schemas.microsoft.com/ado/2009/11/mapping/cs">
  <EntityContainerMapping StorageEntityContainer="SCFAStoreContainer" CdmEntityContainer="SCFAEntities">
    <EntitySetMapping Name="Authentications">
      <EntityTypeMapping TypeName="SCFA.Authentication">
        <MappingFragment StoreEntitySet="Authentication">
          <ScalarProperty Name="username" ColumnName="username" />
          <ScalarProperty Name="pwd" ColumnName="pwd" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Images">
      <EntityTypeMapping TypeName="SCFA.Image">
        <MappingFragment StoreEntitySet="Image">
          <ScalarProperty Name="uname" ColumnName="uname" />
          <ScalarProperty Name="path" ColumnName="path" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="UserDatas">
      <EntityTypeMapping TypeName="SCFA.UserData">
        <MappingFragment StoreEntitySet="UserData">
          <ScalarProperty Name="fname" ColumnName="fname" />
          <ScalarProperty Name="mname" ColumnName="mname" />
          <ScalarProperty Name="lname" ColumnName="lname" />
          <ScalarProperty Name="uname" ColumnName="uname" />
          <ScalarProperty Name="contact" ColumnName="contact" />
          <ScalarProperty Name="bday" ColumnName="bday" />
          <ScalarProperty Name="age" ColumnName="age" />
          <ScalarProperty Name="email" ColumnName="email" />
          <ScalarProperty Name="address" ColumnName="address" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
  </EntityContainerMapping>
</Mapping>